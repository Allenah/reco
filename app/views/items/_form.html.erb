<%= simple_form_for [@list, @item] do |f| %>
    <%= f.association :list, collection: List.all, include_blank: false %>
    <%= f.input :name, :label => "Title" %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.input :description, input_html: {maxlength: 200} %>
    <%= f.submit "Add item", class: "btn btn-primary" %>
<% end %>


